<MudDialog>
    <DialogContent>
        <MudText>Are you sure you want delete this transaction?</MudText>
        <MudText> Name: @DialogModel.Name</MudText>
        <MudText> Date: @DialogModel.Date</MudText>
        <MudText> Price: @DialogModel.Price</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Submit" Variant="Variant.Filled" Color="Color.Primary">Submit</MudButton>
        <MudButton OnClick="Cancel" Variant="Variant.Filled" Color="Color.Secondary">Cancel</MudButton>
    </DialogActions>
</MudDialog>