<MudDialog>
    <DialogContent>
        <MudText>@Localizer["HeaderQuestion"]</MudText>
        <MudText>@Localizer["Name"]: @DialogModel.Name</MudText>
        <MudText>@Localizer["Date"]: @DialogModel.Date</MudText>
        <MudText>@Localizer["Price"]: @DialogModel.Price</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Submit" Variant="Variant.Filled" Color="Color.Primary">@Localizer["Submit"]</MudButton>
        <MudButton OnClick="Cancel" Variant="Variant.Filled" Color="Color.Secondary">@Localizer["Cancel"]</MudButton>
    </DialogActions>
</MudDialog>